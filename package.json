{
  "name": "angular-checklist",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"yarn build-content:watch\"",
    "build": "ng build",
    "build:ssr": "ng build --prod && ng run angular-checklist:server -- --output-hashing=none",
    "prerender": "npm run build:ssr && ts-node prerender.ts",
    "serve:ssr": "npm run prerender && http-server dist/browser",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress open",
    "build-content": "ts-node ./tools/build-checklist.ts",
    "build-content:watch": "nodemon",
    "ci": "npm run build-content && ng build --prod",
    "format:base": "prettier \"{src,tools,cypress}/**/*{.ts,.js,.json,.html}\"",
    "format:check": "npm run format:base -- --list-different",
    "format:fix": "npm run format:base -- --write"
  },
  "engines": {
    "node": "12.22.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tools}/**/*{.ts,.js,.json,.html}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/platform-server": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@ngrx/router-store": "^9.2.1",
    "@ngrx/store": "^9.2.1",
    "@ngrx/store-devtools": "^9.2.1",
    "@types/node": "^12.11.1",
    "core-js": "^2.5.4",
    "fuzzysort": "^1.1.4",
    "hammerjs": "^2.0.8",
    "highlight.js": "^11.3.1",
    "lodash.groupby": "^4.6.0",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-localstorage": "^9.0.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^1.14.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular-devkit/schematics": "9.1.15",
    "@angular/cli": "~9.1.15",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/travis-cli": "^7.2.1",
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^9.0.0-next.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/markdown-it": "^12.2.3",
    "chalk": "^2.4.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^4.1.0",
    "cypress": "^3.1.3",
    "gray-matter": "^4.0.1",
    "http-server": "^0.11.1",
    "husky": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.0",
    "markdown-it": "^12.2.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.2",
    "shorthash": "^0.0.2",
    "ts-loader": "^5.3.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
